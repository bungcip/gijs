{
  "name": "gi",
  "version": "1.0.0",
  "keywords": [
    "util",
    "functional",
    "client",
    "browser"
  ],
  "author": "Gigih Aji Ibrahim",
  "contributors": [],
  "main": "dist/gi.js",
  "typings": "built/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c",
    "clean": "rimraf dist built coverage .nyc_output",
    "lint": "tslint src/**/*.ts",
    "test": "nyc ava --reporter=text --reporter=html",
    "upload-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prebuild": "npm run clean",
    "pretest": "cross-env NODE_ENV=test npm run build",
    "posttest": "npm run lint",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prepublish": "cross-env NODE_ENV=production npm run build"
  },
  "engines": {
    "node": ">=4.4.7",
    "npm": ">=4.4.7"
  },
  "dependencies": {
    "@types/jquery": "^1.10.31",
    "@types/knockout": "^3.4.33",
    "@types/lodash": "^4.14.34"
  },
  "devDependencies": {
    "change-case": "^3.0.0",
    "cross-env": "^2.0.0",
    "rimraf": "2.5.4",
    "rollup": "^0.34.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.3.7",
    "tslint": "^3.15.1",
    "typescript": "^2.0.0"
  }
}